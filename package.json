    "awesome-typescript-loader": "^5.2.1",
    "copyfiles": "^2.1.0",
    "core-js": "^2.6.9",
    "del-cli": "^2.0.0",
    "jasmine-core": "^3.4.0",
    "karma": "^4.1.0",
