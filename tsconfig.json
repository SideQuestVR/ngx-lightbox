{
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  },
  "compileOnSave": false,
  "compilerOptions": {
    "module": "es2020",
    "sourceMap": true,
    "declaration": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "target": "es2015",
    "outDir": "./dist/out-tsc",
    "baseUrl": "./",
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {
      "ngx-lightbox": [
        "dist/ngx-lightbox"
      ],
      "ngx-lightbox/*": [
        "dist/ngx-lightbox/*"
      ]
    },
    "lib": [
      "es2018",
      "dom"
    ]
  }
}